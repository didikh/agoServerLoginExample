<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="iisnode" path="server.js" verb="*" modules="iisnode" resourceType="Unspecified" />
        </handlers>
        <defaultDocument>
            <files>
                <add value="server.js" />
            </files>
        </defaultDocument>
        <rewrite>
            <rules>
                <clear />
                <rule name="appLogin/IISNode" stopProcessing="true">
                    <match url="appLogin/iisnode/*" />
                    <conditions logicalGrouping="MatchAll" trackAllCaptures="false" />
                    <action type="None" />
                </rule>
                <rule name="appLogin" stopProcessing="true">
                    <match url="appLogin/*" />
                    <conditions logicalGrouping="MatchAll" trackAllCaptures="false" />
                    <action type="Rewrite" url="appLogin/server.js" />
                </rule>
                <rule name="userLogin/iisnode" stopProcessing="true">
                    <match url="userLogin/iisnode/*" />
                    <conditions logicalGrouping="MatchAll" trackAllCaptures="false" />
                    <action type="None" />
                </rule>
                <rule name="userLogin" stopProcessing="true">
                    <match url="userLogin/*" />
                    <conditions logicalGrouping="MatchAll" trackAllCaptures="false" />
                    <action type="Rewrite" url="userLogin/server.js" />
                </rule>
                <rule name="root\iisnode" stopProcessing="true">
                    <match url="iisnode/*" />
                    <conditions logicalGrouping="MatchAll" trackAllCaptures="false" />
                    <action type="None" />
                </rule>
                <rule name="root" enabled="true">
                    <match url="/*" />
                    <conditions logicalGrouping="MatchAll" trackAllCaptures="false" />
                    <action type="Rewrite" url="./server.js" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
